#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jeonghoonko/Documents/2025/microservices/microservices-backend/apps/notifications/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules/@nuxt/opencollective/bin/node_modules:/Users/jeonghoonko/Documents/2025/microservices/microservices-backend/apps/notifications/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules/@nuxt/opencollective/node_modules:/Users/jeonghoonko/Documents/2025/microservices/microservices-backend/apps/notifications/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules/@nuxt/node_modules:/Users/jeonghoonko/Documents/2025/microservices/microservices-backend/apps/notifications/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules:/Users/jeonghoonko/Documents/2025/microservices/microservices-backend/apps/notifications/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jeonghoonko/Documents/2025/microservices/microservices-backend/apps/notifications/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules/@nuxt/opencollective/bin/node_modules:/Users/jeonghoonko/Documents/2025/microservices/microservices-backend/apps/notifications/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules/@nuxt/opencollective/node_modules:/Users/jeonghoonko/Documents/2025/microservices/microservices-backend/apps/notifications/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules/@nuxt/node_modules:/Users/jeonghoonko/Documents/2025/microservices/microservices-backend/apps/notifications/node_modules/.pnpm/@nuxt+opencollective@0.4.1/node_modules:/Users/jeonghoonko/Documents/2025/microservices/microservices-backend/apps/notifications/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/opencollective.js" "$@"
else
  exec node  "$basedir/../../bin/opencollective.js" "$@"
fi
